            steps{
                echo("dev deployment")
            }
        }
        
         stage("Deploy to Qa"){
            steps{
                echo("QA Deployment")
            }
        }
        
         stage("Run Automation reg tests"){
            steps{
                echo("Run Automation reg tests")
            }
        }
        
        stage("Deploy to Stage"){
            steps{
                echo("Stage Deployment")
            }
        }
        
        stage("Deplay to PROD"){
            steps{
                echo("PROD Deployment")
            }
        }
    }
    
    
   
    
}